{
  "routes": [
    { "src": "/favicon.ico", "dest": "/papex-favicon.ico" },
    { "src": "/favicon.png", "dest": "/papex-favicon.png" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 